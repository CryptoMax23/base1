  "scripts": {
    "dev": "concurrently \"pnpm run dev:app\" \"pnpm run dev:tunnel\" \"pnpm run dev:pages\"",
    "dev:app": "next dev --turbopack",
    "dev:tunnel": "cloudflared tunnel --config .cloudflared/config.yml run base",
    "dev:pages": "wrangler pages dev --local",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy"
  },
